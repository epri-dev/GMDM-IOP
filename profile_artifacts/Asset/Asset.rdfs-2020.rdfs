      <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
      <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Voltage applied to the winding (end) during test.</rdfs:comment>
      <rdfs:domain rdf:resource="#NoLoadTest"/>
      <rdfs:label xml:lang="en">energisedEndVoltage</rdfs:label>
   </rdf:Description>
