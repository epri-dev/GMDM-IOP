      <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
      <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A connection of energy generation or consumption on the power system model.</rdfs:comment>
      <rdfs:label xml:lang="en">EnergyConnection</rdfs:label>
      <rdfs:subClassOf rdf:resource="#ConductingEquipment"/>
   </rdf:Description>
   <rdf:Description rdf:about="#EnergyConsumer">
      <cims:belongsToCategory rdf:resource="#Package_Wires"/>
      <cims:stereotype rdf:resource="http://iec.ch/TC57/NonStandard/UML#concrete"/>
      <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
      <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Generic user of energy - a  point of consumption on the power system model.
EnergyConsumer.pfixed, .qfixed, .pfixedPct and .qfixedPct have meaning only if there is no LoadResponseCharacteristic associated with EnergyConsumer or if LoadResponseCharacteristic.exponentModel is set to False.</rdfs:comment>
      <rdfs:label xml:lang="en">EnergyConsumer</rdfs:label>
      <rdfs:subClassOf rdf:resource="#EnergyConnection"/>
   </rdf:Description>
   <rdf:Description rdf:about="#EnergyConsumer.EnergyConsumerPhase">
      <cims:AssociationUsed>No</cims:AssociationUsed>
      <cims:inverseRoleName rdf:resource="#EnergyConsumerPhase.EnergyConsumer"/>
      <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..3"/>
      <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
      <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The individual phase models for this energy consumer.</rdfs:comment>
      <rdfs:domain rdf:resource="#EnergyConsumer"/>
      <rdfs:label xml:lang="en">EnergyConsumerPhase</rdfs:label>
      <rdfs:range rdf:resource="#EnergyConsumerPhase"/>
   </rdf:Description>
   <rdf:Description rdf:about="#EnergyConsumer.customerCount">
      <cims:dataType rdf:resource="#Integer"/>
