      <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Conducting equipment have terminals that may be connected to other conducting equipment terminals via connectivity nodes or topological nodes.</rdfs:comment>
      <rdfs:domain rdf:resource="#ConductingEquipment"/>
      <rdfs:label xml:lang="en">Terminals</rdfs:label>
      <rdfs:range rdf:resource="#Terminal"/>
   </rdf:Description>
