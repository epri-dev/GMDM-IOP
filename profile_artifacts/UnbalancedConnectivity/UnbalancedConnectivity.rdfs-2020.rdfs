      <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Combination of conducting material with consistent electrical characteristics, building a single electrical system, used to carry current between points in the power system.</rdfs:comment>
      <rdfs:label xml:lang="en">Conductor</rdfs:label>
      <rdfs:subClassOf rdf:resource="#ConductingEquipment"/>
   </rdf:Description>
   <rdf:Description rdf:about="#ConnectivityNode">
