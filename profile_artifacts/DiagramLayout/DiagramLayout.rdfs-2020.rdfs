      <rdfs:label xml:lang="en">zPosition</rdfs:label>
   </rdf:Description>
   <rdf:Description rdf:about="#DiagramObjectStyle">
      <cims:belongsToCategory rdf:resource="#Package_DiagramLayout"/>
      <cims:stereotype rdf:resource="http://iec.ch/TC57/NonStandard/UML#concrete"/>
      <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
      <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A reference to a style used by the originating system for a diagram object.  A diagram object style describes information such as line thickness, shape such as circle or rectangle etc, and colour.</rdfs:comment>
      <rdfs:label xml:lang="en">DiagramObjectStyle</rdfs:label>
      <rdfs:subClassOf rdf:resource="#IdentifiedObject"/>
   </rdf:Description>
   <rdf:Description rdf:about="#DiagramObjectStyle.StyledObjects">
      <cims:AssociationUsed>No</cims:AssociationUsed>
      <cims:inverseRoleName rdf:resource="#DiagramObject.DiagramObjectStyle"/>
      <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..n"/>
      <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
      <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A style can be assigned to multiple diagram objects.</rdfs:comment>
      <rdfs:domain rdf:resource="#DiagramObjectStyle"/>
      <rdfs:label xml:lang="en">StyledObjects</rdfs:label>
      <rdfs:range rdf:resource="#DiagramObject"/>
   </rdf:Description>
   <rdf:Description rdf:about="#DiagramStyle">
      <cims:belongsToCategory rdf:resource="#Package_DiagramLayout"/>
      <cims:stereotype rdf:resource="http://iec.ch/TC57/NonStandard/UML#concrete"/>
      <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
      <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The diagram style refers to a style used by the originating system for a diagram.  A diagram style describes information such as schematic, geographic, etc.</rdfs:comment>
      <rdfs:label xml:lang="en">DiagramStyle</rdfs:label>
      <rdfs:subClassOf rdf:resource="#IdentifiedObject"/>
   </rdf:Description>
   <rdf:Description rdf:about="#DiagramStyle.Diagram">
