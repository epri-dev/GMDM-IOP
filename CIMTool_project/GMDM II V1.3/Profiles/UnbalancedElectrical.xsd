   <xs:complexType name="PowerElectronicsConnection"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerElectronicsConnection">
      <xs:annotation>
         <xs:documentation>A connection to the AC network for energy production or consumption that uses power electronics rather than rotating machines.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:RegulatingCondEq">
            <xs:sequence>
               <xs:element name="controlMode"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:ConverterControlModeKind"
                           sawsdl:modelReference="http://epri.com/gmdm/2025#PowerElectronicsConnection.controlMode">
                  <xs:annotation>
                     <xs:documentation>Inverter control mode indicating whether reactive power output is based on constant PF or constant Q</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="maxQ"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:ReactivePower"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerElectronicsConnection.maxQ">
                  <xs:annotation>
                     <xs:documentation>Maximum reactive power limit. This is the maximum (nameplate) limit for the unit.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="minQ"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:ReactivePower"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerElectronicsConnection.minQ">
                  <xs:annotation>
                     <xs:documentation>Minimum reactive power limit for the unit. This is the minimum (nameplate) limit for the unit.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="ratedS"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:ApparentPower"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerElectronicsConnection.ratedS">
                  <xs:annotation>
                     <xs:documentation>Nameplate apparent power rating for the unit.</xs:documentation>
                     <xs:documentation>The attribute shall have a positive value.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="ratedU"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Voltage"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerElectronicsConnection.ratedU">
                  <xs:annotation>
                     <xs:documentation>Rated voltage (nameplate data, Ur in IEC 60909-0). It is primarily used for short circuit data exchange according to IEC 60909.</xs:documentation>
                     <xs:documentation>The attribute shall be a positive value.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="PowerElectronicsThermalUnit"
                   sawsdl:modelReference="http://epri.com/gmdm/2025#PowerElectronicsThermalUnit">
      <xs:annotation>
         <xs:documentation>A thermal unit that is connected via an inverter such as through a back-to-back converter</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:PowerElectronicsUnit">
            <xs:sequence>
               <xs:element name="FossilFuels"
                           minOccurs="1"
                           maxOccurs="1"
                           sawsdl:modelReference="http://epri.com/gmdm/2025#PowerElectronicsThermalUnit.FossilFuels">
                  <xs:annotation/>
                  <xs:complexType sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#FossilFuel">
                     <xs:attribute name="ref" type="xs:string" use="required"/>
                     <xs:attribute name="referenceType" type="xs:string" use="optional"/>
                  </xs:complexType>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="PowerElectronicsUnit"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerElectronicsUnit">
      <xs:annotation>
         <xs:documentation>A generating unit or battery or aggregation that connects to the AC network using power electronics rather than rotating machines.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:Equipment">
            <xs:sequence>
               <xs:element name="maxP"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:ActivePower"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerElectronicsUnit.maxP">
                  <xs:annotation>
                     <xs:documentation>Maximum active power limit. This is the maximum (nameplate) limit for the unit.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="minP"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:ActivePower"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerElectronicsUnit.minP">
                  <xs:annotation>
                     <xs:documentation>Minimum active power limit. This is the minimum (nameplate) limit for the unit.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="PowerElectronicsConnection"
                           minOccurs="0"
                           maxOccurs="1"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerElectronicsUnit.PowerElectronicsConnection">
                  <xs:annotation>
                     <xs:documentation>A power electronics unit has a connection to the AC network.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerElectronicsConnection">
                     <xs:attribute name="ref" type="xs:string" use="required"/>
                     <xs:attribute name="referenceType" type="xs:string" use="optional"/>
                  </xs:complexType>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="PowerElectronicsWindUnit"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerElectronicsWindUnit">
      <xs:annotation>
         <xs:documentation>A wind generating unit that connects to the AC network with power electronics rather than rotating machines or an aggregation of such units.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:PowerElectronicsUnit">
            <xs:sequence/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="PowerSystemResource"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerSystemResource">
      <xs:annotation>
         <xs:documentation>A power system resource (PSR) can be an item of equipment such as a switch, an equipment container containing many individual items of equipment such as a substation, or an organisational entity such as sub-control area. Power system resources can have measurements associated.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:IdentifiedObject">
            <xs:sequence/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="PowerTransformer"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerTransformer">
      <xs:annotation>
         <xs:documentation>An electrical device consisting of  two or more coupled windings, with or without a magnetic core, for introducing mutual coupling between electric circuits. Transformers can be used to control voltage and phase shift (active power flow).</xs:documentation>
         <xs:documentation>A power transformer may be composed of separate transformer tanks that need not be identical.</xs:documentation>
         <xs:documentation>A power transformer can be modelled with or without tanks and is intended for use in both balanced and unbalanced representations. A power transformer typically has two terminals, but may have one (grounding), three or more terminals.</xs:documentation>
         <xs:documentation>The inherited association ConductingEquipment.BaseVoltage should not be used. The association from TransformerEnd to BaseVoltage should be used instead.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:ConductingEquipment">
            <xs:sequence/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="PowerTransformerEnd"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerTransformerEnd">
      <xs:annotation>
         <xs:documentation>A PowerTransformerEnd is associated with each Terminal of a PowerTransformer.</xs:documentation>
         <xs:documentation>The impedance values r, r0, x, and x0 of a PowerTransformerEnd represents a star equivalent as follows.</xs:documentation>
         <xs:documentation>1) two PowerTransformerEnd-s shall be defined for a two Terminal PowerTransformer even if the two PowerTransformerEnd-s have the same rated voltage. The high voltage PowerTransformerEnd (TransformerEnd.endNumber=1) is the one used to exchange resistances (r, r0) and reactances (x, x0) of the PowerTransformer while the low voltage PowerTransformerEnd (TransformerEnd.endNumber=2) shall have zero impedance values.</xs:documentation>
         <xs:documentation>2) for a three Terminal PowerTransformer the three PowerTransformerEnds represent a star equivalent with each leg in the star represented by r, r0, x, and x0 values.</xs:documentation>
         <xs:documentation>3) For a three Terminal transformer each PowerTransformerEnd shall have g, g0, b and b0 values corresponding to the no load losses distributed on the three PowerTransformerEnds. The total no load loss shunt impedances may also be placed at one of the PowerTransformerEnds, preferably the end numbered 1, having the shunt values on end 1.  This is the preferred way.</xs:documentation>
         <xs:documentation>4) for a PowerTransformer with more than three Terminals the PowerTransformerEnd impedance values cannot be used. Instead use the TransformerMeshImpedance or split the transformer into multiple PowerTransformers.</xs:documentation>
         <xs:documentation>Each PowerTransformerEnd must be contained by a PowerTransformer. Because a PowerTransformerEnd (or any other object) can not be contained by more than one parent, a PowerTransformerEnd can not have an association to an EquipmentContainer (Substation, VoltageLevel, etc).</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:TransformerEnd">
            <xs:sequence>
               <xs:element name="b"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Susceptance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerTransformerEnd.b">
                  <xs:annotation>
                     <xs:documentation>Magnetizing branch susceptance (B mag).  The value can be positive or negative.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="g"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Conductance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerTransformerEnd.g">
                  <xs:annotation>
                     <xs:documentation>Magnetizing branch conductance.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="phaseAngleClock"
                           minOccurs="0"
                           maxOccurs="1"
                           type="xs:integer"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerTransformerEnd.phaseAngleClock">
                  <xs:annotation>
                     <xs:documentation>Terminal voltage phase angle displacement where 360 degrees are represented with clock hours. The valid values are 0 to 11. For example, for the secondary side end of a transformer with vector group code of 'Dyn11', specify the connection kind as wye with neutral and specify the phase angle of the clock as 11.  The clock value of the transformer end number specified as 1, is assumed to be zero.  Note the transformer end number is not assumed to be the same as the terminal sequence number.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="r"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:Resistance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerTransformerEnd.r">
                  <xs:annotation>
                     <xs:documentation>Resistance (star-model) of the transformer end.</xs:documentation>
                     <xs:documentation>The attribute shall be equal to or greater than zero for non-equivalent transformers.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="ratedS"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:ApparentPower"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerTransformerEnd.ratedS">
                  <xs:annotation>
                     <xs:documentation>Normal apparent power rating.</xs:documentation>
                     <xs:documentation>The attribute shall be a positive value. For a two-winding transformer the values for the high and low voltage sides shall be identical.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="ratedU"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:Voltage"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerTransformerEnd.ratedU">
                  <xs:annotation>
                     <xs:documentation>Rated voltage: phase-phase for three-phase windings, and either phase-phase or phase-neutral for single-phase windings.</xs:documentation>
                     <xs:documentation>A high voltage side, as given by TransformerEnd.endNumber, shall have a ratedU that is greater than or equal to ratedU for the lower voltage sides.</xs:documentation>
                     <xs:documentation>The attribute shall be a positive value.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="x"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Reactance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PowerTransformerEnd.x">
                  <xs:annotation>
                     <xs:documentation>Positive sequence series reactance (star-model) of the transformer end.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="ProtectedSwitch"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#ProtectedSwitch">
      <xs:annotation>
         <xs:documentation>A ProtectedSwitch is a switching device that can be operated by ProtectionEquipment.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:Switch">
            <xs:sequence>
               <xs:element name="breakingCapacity"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:CurrentFlow"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#ProtectedSwitch.breakingCapacity">
                  <xs:annotation>
                     <xs:documentation>The maximum fault current a breaking device can break safely under prescribed conditions of use.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="RatioTapChanger"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RatioTapChanger">
      <xs:annotation>
         <xs:documentation>A tap changer that changes the voltage ratio impacting the voltage magnitude but not the phase angle across the transformer.</xs:documentation>
         <xs:documentation>Angle sign convention (general): Positive value indicates a positive phase shift from the winding where the tap is located to the other winding (for a two-winding transformer).</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:TapChanger">
            <xs:sequence>
               <xs:element name="stepVoltageIncrement"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:PerCent"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RatioTapChanger.stepVoltageIncrement">
                  <xs:annotation>
                     <xs:documentation>Tap step increment, in per cent of rated voltage of the power transformer end, per step position.</xs:documentation>
                     <xs:documentation>When the increment is negative, the voltage decreases when the tap step increases.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="TransformerEnd"
                           minOccurs="1"
                           maxOccurs="1"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RatioTapChanger.TransformerEnd">
                  <xs:annotation>
                     <xs:documentation>Transformer end to which this ratio tap changer belongs.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TransformerEnd">
                     <xs:attribute name="ref" type="xs:string" use="required"/>
                     <xs:attribute name="referenceType" type="xs:string" use="optional"/>
                  </xs:complexType>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="Recloser"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#Recloser">
      <xs:annotation>
         <xs:documentation>Pole-mounted fault interrupter with built-in phase and ground relays, current transformer (CT), and supplemental controls.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:ProtectedSwitch">
            <xs:sequence/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="RegulatingCondEq"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RegulatingCondEq">
      <xs:annotation>
         <xs:documentation>A type of conducting equipment that can regulate a quantity (i.e. voltage or flow) at a specific point in the network.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:EnergyConnection">
            <xs:sequence>
               <xs:element name="RegulatingControl"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:RegulatingControl"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RegulatingCondEq.RegulatingControl">
                  <xs:annotation>
                     <xs:documentation>The regulating control scheme in which this equipment participates.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="RegulatingControl"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RegulatingControl">
      <xs:annotation>
         <xs:documentation>Specifies a set of equipment that works together to control a power system quantity such as voltage or flow.</xs:documentation>
         <xs:documentation>Remote bus voltage control is possible by specifying the controlled terminal located at some place remote from the controlling equipment.</xs:documentation>
         <xs:documentation>The specified terminal shall be associated with the connectivity node of the controlled point.  The most specific subtype of RegulatingControl shall be used in case such equipment participate in the control, e.g. TapChangerControl for tap changers.</xs:documentation>
         <xs:documentation>For flow control, load sign convention is used, i.e. positive sign means flow out from a TopologicalNode (bus) into the conducting equipment.</xs:documentation>
         <xs:documentation>The attribute minAllowedTargetValue and maxAllowedTargetValue are required in the following cases:</xs:documentation>
         <xs:documentation>- For a power generating module operated in power factor control mode to specify maximum and minimum power factor values;</xs:documentation>
         <xs:documentation>- Whenever it is necessary to have an off center target voltage for the tap changer regulator. For instance, due to long cables to off shore wind farms and the need to have a simpler setup at the off shore transformer platform, the voltage is controlled from the land at the connection point for the off shore wind farm. Since there usually is a voltage rise along the cable, there is typical and overvoltage of up 3 to 4 kV compared to the on shore station. Thus in normal operation the tap changer on the on shore station is operated with a target set point, which is in the lower parts of the dead band.</xs:documentation>
         <xs:documentation>The attributes minAllowedTargetValue and maxAllowedTargetValue are not related to the attribute targetDeadband and thus they are not treated as an alternative of the targetDeadband. They are needed due to limitations in the local substation controller. The attribute targetDeadband is used to prevent the power flow from moving the tap position in circles (hunting) that is to be used regardless of the attributes minAllowedTargetValue and maxAllowedTargetValue.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:PowerSystemResource">
            <xs:sequence>
               <xs:element name="discrete"
                           minOccurs="1"
                           maxOccurs="1"
                           type="xs:boolean"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RegulatingControl.discrete">
                  <xs:annotation>
                     <xs:documentation>If true, indicates that regulation is performed in the simulation in a discrete mode. If false, indicates that non-discrete regulation is used in the simulation. This applies to equipment with discrete controls, e.g. tap changers and shunt compensators.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="enabled"
                           minOccurs="1"
                           maxOccurs="1"
                           type="xs:boolean"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RegulatingControl.enabled">
                  <xs:annotation>
                     <xs:documentation>The flag tells if regulation is enabled.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="mode"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:RegulatingControlModeKind"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RegulatingControl.mode">
                  <xs:annotation>
                     <xs:documentation>The regulating control mode presently available.  This specification allows for determining the kind of regulation without need for obtaining the units from a schedule.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="monitoredPhase"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:PhaseCode"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RegulatingControl.monitoredPhase">
                  <xs:annotation>
                     <xs:documentation>Phase voltage controlling this regulator, measured at regulator location.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="targetDeadband"
                           minOccurs="0"
                           maxOccurs="1"
                           type="xs:float"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RegulatingControl.targetDeadband">
                  <xs:annotation>
                     <xs:documentation>This is the deadband set on the controller. It is used with discrete control to avoid excessive tap changes in tap changers and state changes in shunt compensator banks while regulating.  The units in which deadband is expressed are based on RegulatingControl.mode. The attribute shall be a positive value or zero. If RegulatingControl.discrete is set to "false", the RegulatingControl.targetDeadband is to be ignored in simulations. If TapChanger.ctRatio or TapChanger.ptRatio are populated, deadband shall be expressed in secondary units.</xs:documentation>
                     <xs:documentation>Note that for instance, if the targetValue is 100 kV and the targetDeadband is 2 kV the range is from 99 to 101 kV.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="targetValue"
                           minOccurs="0"
                           maxOccurs="1"
                           type="xs:float"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RegulatingControl.targetValue">
                  <xs:annotation>
                     <xs:documentation>The target value set on the controller. It is the starting value for a steady state solution. This value can be used for the target value without the use of schedules. The value has the units appropriate to the mode attribute.</xs:documentation>
                     <xs:documentation>If used for a TapChangerControl and if TapChanger.ctRatio or TapChanger.ptRatio are populated, the target value shall be expressed in secondary units. As an example, with PT ratio of 60 on a 12.47 KV distribution system, a phase to ground PT has a ratio of 7200 volts primary to 120 volts secondary. If the required primary target is 7128 volts for a single phase tap changer then the value of targetValue would equal 118.8 volts secondary (7128/60).</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="Terminal"
                           minOccurs="1"
                           maxOccurs="1"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RegulatingControl.Terminal">
                  <xs:annotation>
                     <xs:documentation>The terminal associated with this regulating control.  The terminal is associated instead of a node, since the terminal could connect into either a topological node or a connectivity node.  Sometimes it is useful to model regulation at a terminal of a bus bar object.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#Terminal">
                     <xs:attribute name="ref" type="xs:string" use="required"/>
                     <xs:attribute name="referenceType" type="xs:string" use="optional"/>
                  </xs:complexType>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:simpleType name="RegulatingControlModeKind"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RegulatingControlModeKind">
      <xs:annotation>
         <xs:documentation>The kind of regulation model.   For example regulating voltage, reactive power, active power, etc.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="activePower">
            <xs:annotation>
               <xs:documentation>Active power is specified.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="admittance">
            <xs:annotation>
               <xs:documentation>Admittance is specified.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="currentFlow">
            <xs:annotation>
               <xs:documentation>Current flow is specified.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="powerFactor">
            <xs:annotation>
               <xs:documentation>Power factor is specified.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="reactivePower">
            <xs:annotation>
               <xs:documentation>Reactive power is specified.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="temperature">
            <xs:annotation>
               <xs:documentation>Control switches on/off based on the local temperature (i.e., a thermostat).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="timeScheduled">
            <xs:annotation>
               <xs:documentation>Control switches on/off by time of day. The times may change on the weekend, or in different seasons.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="voltage">
            <xs:annotation>
               <xs:documentation>Voltage is specified.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:complexType name="RotatingMachine"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RotatingMachine">
      <xs:annotation>
         <xs:documentation>A rotating machine which may be used as a generator or motor.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:RegulatingCondEq">
            <xs:sequence>
               <xs:element name="ratedS"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:ApparentPower"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RotatingMachine.ratedS">
                  <xs:annotation>
                     <xs:documentation>Nameplate apparent power rating for the unit.</xs:documentation>
                     <xs:documentation>The attribute shall have a positive value.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="ratedU"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:Voltage"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RotatingMachine.ratedU">
                  <xs:annotation>
                     <xs:documentation>Rated voltage (nameplate data, Ur in IEC 60909-0). It is primarily used for short circuit data exchange according to IEC 60909.</xs:documentation>
                     <xs:documentation>The attribute shall be a positive value.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="GeneratingUnit"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:GeneratingUnit"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RotatingMachine.GeneratingUnit">
                  <xs:annotation>
                     <xs:documentation>A synchronous machine may operate as a generator and as such becomes a member of a generating unit.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="Sectionaliser"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#Sectionaliser">
      <xs:annotation>
         <xs:documentation>Automatic switch that will lock open to isolate a faulted section. It may, or may not, have load breaking capability. Its primary purpose is to provide fault sectionalising at locations where the fault current is either too high, or too low, for proper coordination of fuses.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:Switch">
            <xs:sequence/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="SeriesCompensator"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#SeriesCompensator">
      <xs:annotation>
         <xs:documentation>A Series Compensator is a series capacitor or reactor or an AC transmission line without charging susceptance.  It is a two terminal device.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:ConductingEquipment">
            <xs:sequence>
               <xs:element name="r"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:Resistance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#SeriesCompensator.r">
                  <xs:annotation>
                     <xs:documentation>Positive sequence resistance.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="x"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:Reactance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#SeriesCompensator.x">
                  <xs:annotation>
                     <xs:documentation>Positive sequence reactance.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="ShuntCompensator"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#ShuntCompensator">
      <xs:annotation>
         <xs:documentation>A shunt capacitor or reactor or switchable bank of shunt capacitors or reactors. A section of a shunt compensator is an individual capacitor or reactor. A negative value for bPerSection indicates that the compensator is a reactor. ShuntCompensator is a single terminal device.  Ground is implied.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:RegulatingCondEq">
            <xs:sequence>
               <xs:element name="aVRDelay"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Seconds"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#ShuntCompensator.aVRDelay">
                  <xs:annotation>
                     <xs:documentation>An automatic voltage regulation delay (AVRDelay) which is the time delay from a change in voltage to when the capacitor is allowed to change state. This filters out temporary changes in voltage.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="maximumSections"
                           minOccurs="1"
                           maxOccurs="1"
                           type="xs:integer"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#ShuntCompensator.maximumSections">
                  <xs:annotation>
                     <xs:documentation>The maximum number of sections that may be switched in.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="nomU"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:Voltage"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#ShuntCompensator.nomU">
                  <xs:annotation>
                     <xs:documentation>The voltage at which the nominal reactive power may be calculated. This should normally be within 10% of the voltage at which the capacitor is connected to the network.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="normalSections"
                           minOccurs="1"
                           maxOccurs="1"
                           type="xs:integer"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#ShuntCompensator.normalSections">
                  <xs:annotation>
                     <xs:documentation>The normal number of sections switched in. The value shall be between zero and ShuntCompensator.maximumSections.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="ShuntCompensatorPhase"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#ShuntCompensatorPhase">
      <xs:annotation>
         <xs:documentation>Single phase of a multi-phase shunt compensator when its attributes might be different per phase.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:PowerSystemResource">
            <xs:sequence>
               <xs:element name="maximumSections"
                           minOccurs="1"
                           maxOccurs="1"
                           type="xs:integer"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#ShuntCompensatorPhase.maximumSections">
                  <xs:annotation>
                     <xs:documentation>The maximum number of sections that may be switched in for this phase.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="normalSections"
                           minOccurs="1"
                           maxOccurs="1"
                           type="xs:integer"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#ShuntCompensatorPhase.normalSections">
                  <xs:annotation>
                     <xs:documentation>For the capacitor phase, the normal number of sections switched in. The value shall be between zero and ShuntCompensatorPhase.maximumSections.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="Switch"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#Switch">
      <xs:annotation>
         <xs:documentation>A generic device designed to close, or open, or both, one or more electric circuits.  All switches are two terminal devices including grounding switches. The ACDCTerminal.connected at the two sides of the switch shall not be considered for assessing switch connectivity, i.e. only Switch.open, .normalOpen and .locked are relevant.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:ConductingEquipment">
            <xs:sequence>
               <xs:element name="ratedCurrent"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:CurrentFlow"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#Switch.ratedCurrent">
                  <xs:annotation>
                     <xs:documentation>The maximum continuous current carrying capacity in amps governed by the device material and construction.</xs:documentation>
                     <xs:documentation>The attribute shall be a positive value.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="SynchronousMachine"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#SynchronousMachine">
      <xs:annotation>
         <xs:documentation>An electromechanical device that operates with shaft rotating synchronously with the network. It is a single machine operating either as a generator or synchronous condenser or pump.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:RotatingMachine">
            <xs:sequence/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="TapChanger"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChanger">
      <xs:annotation>
         <xs:documentation>Mechanism for changing transformer winding tap positions.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:PowerSystemResource">
            <xs:sequence>
               <xs:element name="controlEnabled"
                           minOccurs="0"
                           maxOccurs="1"
                           type="xs:boolean"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChanger.controlEnabled">
                  <xs:annotation>
                     <xs:documentation>Specifies the regulation status of the equipment.  True is regulating, false is not regulating.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="ctRating"
                           minOccurs="0"
                           maxOccurs="1"
                           type="xs:float"
                           sawsdl:modelReference="http://epri.com/gmdm/2025#TapChanger.ctRating">
                  <xs:annotation/>
               </xs:element>
               <xs:element name="ctRatio"
                           minOccurs="0"
                           maxOccurs="1"
                           type="xs:float"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChanger.ctRatio">
                  <xs:annotation>
                     <xs:documentation>Built-in current transformer ratio. If a value is specified, then RegulatingControl.targetValue (and RegulatingControl.targetDeadband, RegulatingControl.maxAllowedTargetValue and RegulatingControl.minAllowedTargetValue) are expressed in secondary units.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="highStep"
                           minOccurs="1"
                           maxOccurs="1"
                           type="xs:integer"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChanger.highStep">
                  <xs:annotation>
                     <xs:documentation>Highest possible tap step position, advance from neutral.</xs:documentation>
                     <xs:documentation>The attribute shall be greater than lowStep.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="initialDelay"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Seconds"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChanger.initialDelay">
                  <xs:annotation>
                     <xs:documentation>For an LTC, the delay for initial tap changer operation (first step change).</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="lowStep"
                           minOccurs="1"
                           maxOccurs="1"
                           type="xs:integer"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChanger.lowStep">
                  <xs:annotation>
                     <xs:documentation>Lowest possible tap step position, retard from neutral.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="ltcFlag"
                           minOccurs="1"
                           maxOccurs="1"
                           type="xs:boolean"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChanger.ltcFlag">
                  <xs:annotation>
                     <xs:documentation>Specifies whether or not a TapChanger has load tap changing capabilities.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="neutralStep"
                           minOccurs="1"
                           maxOccurs="1"
                           type="xs:integer"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChanger.neutralStep">
                  <xs:annotation>
                     <xs:documentation>The neutral tap step position for this winding.</xs:documentation>
                     <xs:documentation>The attribute shall be equal to or greater than lowStep and equal or less than highStep.</xs:documentation>
                     <xs:documentation>It is the step position where the voltage is neutralU when the other terminals of the transformer are at the ratedU. If there are other tap changers on the transformer those taps are kept constant at their neutralStep.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="neutralU"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:Voltage"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChanger.neutralU">
                  <xs:annotation>
                     <xs:documentation>Voltage at which the winding operates at the neutral tap setting. It is the voltage at the terminal of the PowerTransformerEnd associated with the tap changer when all tap changers on the transformer are at their neutralStep position.  Normally neutralU of the tap changer is the same as ratedU of the PowerTransformerEnd, but it can differ in special cases such as when the tapping mechanism is separate from the winding more common on lower voltage transformers.</xs:documentation>
                     <xs:documentation>This attribute is not relevant for PhaseTapChangerAsymmetrical, PhaseTapChangerSymmetrical and PhaseTapChangerLinear.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="ptRatio"
                           minOccurs="0"
                           maxOccurs="1"
                           type="xs:float"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChanger.ptRatio">
                  <xs:annotation>
                     <xs:documentation>Built-in potential (voltage) transformer ratio. If a value is specified, then RegulatingControl.targetValue (and RegulatingControl.targetDeadband, RegulatingControl.maxAllowedTargetValue, RegulatingControl.minAllowedTargetValue as well as TapChangerControl.maxLimitVoltage and TapChangerControl.minLimitVoltage) are expressed in secondary units.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="subsequentDelay"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Seconds"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChanger.subsequentDelay">
                  <xs:annotation>
                     <xs:documentation>For an LTC, the delay for subsequent tap changer operation (second and later step changes).</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="TapChangerControl"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:TapChangerControl"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChanger.TapChangerControl">
                  <xs:annotation>
                     <xs:documentation>The regulating control scheme in which this tap changer participates.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="TapChangerControl"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChangerControl">
      <xs:annotation>
         <xs:documentation>Describes behaviour specific to tap changers, e.g. how the voltage at the end of a line varies with the load level and compensation of the voltage drop by tap adjustment. When TapChanger.ctRatio and .ptRatio are present, RegulatingControl.targetVoltage RegulatingControl.targetDeadband, RegulatingControl.maxAllowedTargetValue, RegulatingControl.minAllowedTargetValue as well as TapChangerControl.maxLimitVoltage and TapChangerControl.minLimitVoltage  shall be expressed in terms of secondary CT currents and PT voltages.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:RegulatingControl">
            <xs:sequence>
               <xs:element name="lineDropCompensation"
                           minOccurs="0"
                           maxOccurs="1"
                           type="xs:boolean"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChangerControl.lineDropCompensation">
                  <xs:annotation>
                     <xs:documentation>If true, the line drop compensation is to be applied.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="lineDropR"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Resistance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChangerControl.lineDropR">
                  <xs:annotation>
                     <xs:documentation>Line drop compensator resistance setting for normal (forward) power flow.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="lineDropX"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Reactance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChangerControl.lineDropX">
                  <xs:annotation>
                     <xs:documentation>Line drop compensator reactance setting for normal (forward) power flow.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="maxLimitVoltage"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Voltage"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChangerControl.maxLimitVoltage">
                  <xs:annotation>
                     <xs:documentation>Maximum allowed regulated voltage on the PT secondary. This is the locally measured secondary voltage, including the effect of any line drop compensation. Typically used for distribution circuit voltage regulator. Sometimes referred to as first-house protection. If utilized, then TapChanger.ptRatio must be supplied.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="minLimitVoltage"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Voltage"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChangerControl.minLimitVoltage">
                  <xs:annotation>
                     <xs:documentation>Minimum allowed regulated voltage on the PT secondary.  This is the locally measured secondary voltage, including the effect of any line drop compensation. Typically used for distribution circuit voltage regulator. Sometimes referred to as last-house protection. If utilized, then TapChanger.ptRatio must be supplied.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="reverseLineDropR"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Resistance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChangerControl.reverseLineDropR">
                  <xs:annotation>
                     <xs:documentation>Line drop compensator resistance setting for reverse power flow.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="reverseLineDropX"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Reactance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TapChangerControl.reverseLineDropX">
                  <xs:annotation>
                     <xs:documentation>Line drop compensator reactance setting for reverse power flow.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="reverseTargetDeadband"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Voltage"
                           sawsdl:modelReference="http://epri.com/gmdm/2025#TapChangerControl.reverseTargetDeadband">
                  <xs:annotation/>
               </xs:element>
               <xs:element name="reverseTargetValue"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Voltage"
                           sawsdl:modelReference="http://epri.com/gmdm/2025#TapChangerControl.reverseTargetValue">
                  <xs:annotation/>
               </xs:element>
               <xs:element name="reverseToNeutral"
                           minOccurs="0"
                           maxOccurs="1"
                           type="xs:boolean"
                           sawsdl:modelReference="http://epri.com/gmdm/2025#TapChangerControl.reverseToNeutral">
                  <xs:annotation/>
               </xs:element>
               <xs:element name="reversible"
                           minOccurs="0"
                           maxOccurs="1"
                           type="xs:boolean"
                           sawsdl:modelReference="http://epri.com/gmdm/2025#TapChangerControl.reversible">
                  <xs:annotation/>
               </xs:element>
               <xs:element name="reversingDelay"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Seconds"
                           sawsdl:modelReference="http://epri.com/gmdm/2025#TapChangerControl.reversingDelay">
                  <xs:annotation/>
               </xs:element>
               <xs:element name="reversingPowerThreshold"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:ApparentPower"
                           sawsdl:modelReference="http://epri.com/gmdm/2025#TapChangerControl.reversingPowerThreshold">
                  <xs:annotation/>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="Terminal"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#Terminal">
      <xs:annotation>
         <xs:documentation>An AC electrical connection point to a piece of conducting equipment. Terminals are connected at physical connection points called connectivity nodes.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:ACDCTerminal">
            <xs:sequence/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="ThermalGeneratingUnit"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#ThermalGeneratingUnit">
      <xs:annotation>
         <xs:documentation>A generating unit whose prime mover could be a steam turbine, combustion turbine, or diesel engine.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:GeneratingUnit">
            <xs:sequence/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="TransformerCoreAdmittance"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TransformerCoreAdmittance">
      <xs:annotation>
         <xs:documentation>The transformer core admittance.  Used to specify the core admittance of a transformer in a manner that can be shared among power transformers.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:IdentifiedObject">
            <xs:sequence>
               <xs:element name="b"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:Susceptance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TransformerCoreAdmittance.b">
                  <xs:annotation>
                     <xs:documentation>Magnetizing branch susceptance (B mag).  The value can be positive or negative.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="g"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:Conductance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TransformerCoreAdmittance.g">
                  <xs:annotation>
                     <xs:documentation>Magnetizing branch conductance (G mag).</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="TransformerEnd"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TransformerEnd">
      <xs:annotation>
         <xs:documentation>A conducting connection point of a power transformer. It corresponds to a physical transformer winding terminal. In earlier CIM versions, the TransformerWinding class served a similar purpose, but this class is more flexible because it associates to terminal but is not a specialization of ConductingEquipment.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:IdentifiedObject">
            <xs:sequence>
               <xs:element name="grounded"
                           minOccurs="1"
                           maxOccurs="1"
                           type="xs:boolean"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TransformerEnd.grounded">
                  <xs:annotation>
                     <xs:documentation>Used only for Yn and Zn connections indicated by PowerTransformerEnd.connectionKind. If true, the neutral is grounded and attributes TransformerEnd.rground and TransformerEnd.xground are required. If false, the attributes TransformerEnd.rground and TransformerEnd.xground are not considered.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="rground"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Resistance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TransformerEnd.rground">
                  <xs:annotation>
                     <xs:documentation>Resistance part of neutral impedance. Zero indicates solidly grounded or grounded through a reactor.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="xground"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Reactance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TransformerEnd.xground">
                  <xs:annotation>
                     <xs:documentation>Reactance part of neutral impedance. Zero indicates solidly grounded or grounded through a reactor.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="CoreAdmittance"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:TransformerCoreAdmittance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TransformerEnd.CoreAdmittance">
                  <xs:annotation>
                     <xs:documentation>Core admittance of this transformer end, representing magnetising current and core losses. The full values of the transformer should be supplied for one transformer end only.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="TransformerMeshImpedance"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TransformerMeshImpedance">
      <xs:annotation>
         <xs:documentation>Transformer mesh impedance (Delta-model) between transformer ends.</xs:documentation>
         <xs:documentation>The typical case is that this class describes the impedance between two transformer ends pair-wise, i.e. the cardinalities at both transformer end associations are 1. However, in cases where two or more transformer ends are modelled the cardinalities are larger than 1.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:IdentifiedObject">
            <xs:sequence>
               <xs:element name="r"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:Resistance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TransformerMeshImpedance.r">
                  <xs:annotation>
                     <xs:documentation>Resistance between the 'from' and the 'to' end, seen from the 'from' end.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="x"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:Reactance"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TransformerMeshImpedance.x">
                  <xs:annotation>
                     <xs:documentation>Reactance between the 'from' and the 'to' end, seen from the 'from' end.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="FromTransformerEnd"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:TransformerEnd"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TransformerMeshImpedance.FromTransformerEnd">
                  <xs:annotation>
                     <xs:documentation>From end this mesh impedance is connected to. It determines the voltage reference.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="ToTransformerEnd"
                           minOccurs="0"
                           maxOccurs="unbounded"
                           type="m:TransformerEnd"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TransformerMeshImpedance.ToTransformerEnd">
                  <xs:annotation>
                     <xs:documentation>All transformer ends this mesh impedance is connected to.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="TransformerTankEnd"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#TransformerTankEnd">
      <xs:annotation>
         <xs:documentation>Transformer tank end represents an individual winding for unbalanced models or for transformer tanks connected into a bank (and bank is modelled with the PowerTransformer).</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:TransformerEnd">
            <xs:sequence/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:simpleType name="WindGenUnitKind"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#WindGenUnitKind">
      <xs:annotation>
         <xs:documentation>Kind of wind generating unit.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="offshore">
            <xs:annotation>
               <xs:documentation>The wind generating unit is located offshore.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="onshore">
            <xs:annotation>
               <xs:documentation>The wind generating unit is located onshore.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:complexType name="WindGeneratingUnit"
                   sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#WindGeneratingUnit">
      <xs:annotation>
         <xs:documentation>A wind driven generating unit, connected to the grid by means of a rotating machine.  May be used to represent a single turbine or an aggregation.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:GeneratingUnit">
            <xs:sequence>
               <xs:element name="windGenUnitType"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:WindGenUnitKind"
                           sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#WindGeneratingUnit.windGenUnitType">
                  <xs:annotation>
                     <xs:documentation>The kind of wind generating unit.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:simpleType name="ActivePower"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#ActivePower">
      <xs:annotation>
         <xs:documentation>Product of RMS value of the voltage and the RMS value of the in-phase component of the current.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:float"/>
   </xs:simpleType>
   <xs:simpleType name="ApparentPower"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#ApparentPower">
      <xs:annotation>
         <xs:documentation>Product of the RMS value of the voltage and the RMS value of the current.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:float"/>
   </xs:simpleType>
   <xs:simpleType name="Conductance"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#Conductance">
      <xs:annotation>
         <xs:documentation>Factor by which voltage must be multiplied to give corresponding power lost from a circuit. Real part of admittance.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:float"/>
   </xs:simpleType>
   <xs:simpleType name="CurrentFlow"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#CurrentFlow">
      <xs:annotation>
         <xs:documentation>Electrical current with sign convention: positive flow is out of the conducting equipment into the connectivity node. Can be both AC and DC.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:float"/>
   </xs:simpleType>
   <xs:simpleType name="Length"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#Length">
      <xs:annotation>
         <xs:documentation>Unit of length. It shall be a positive value or zero.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:float"/>
   </xs:simpleType>
   <xs:simpleType name="PerCent"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#PerCent">
      <xs:annotation>
         <xs:documentation>Percentage on a defined base. For example, specify as 100 to indicate at the defined base.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:float"/>
   </xs:simpleType>
   <xs:simpleType name="Reactance"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#Reactance">
      <xs:annotation>
         <xs:documentation>Reactance (imaginary part of impedance), at rated frequency.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:float"/>
   </xs:simpleType>
   <xs:simpleType name="ReactancePerLength"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#ReactancePerLength">
      <xs:annotation>
         <xs:documentation>Reactance (imaginary part of impedance) per unit of length, at rated frequency.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:float"/>
   </xs:simpleType>
   <xs:simpleType name="ReactivePower"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#ReactivePower">
      <xs:annotation>
         <xs:documentation>Product of RMS value of the voltage and the RMS value of the quadrature component of the current.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:float"/>
   </xs:simpleType>
   <xs:simpleType name="RealEnergy"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#RealEnergy">
      <xs:annotation>
         <xs:documentation>Real electrical energy.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:float"/>
   </xs:simpleType>
   <xs:simpleType name="Resistance"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#Resistance">
      <xs:annotation>
         <xs:documentation>Resistance (real part of impedance).</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:float"/>
   </xs:simpleType>
   <xs:simpleType name="ResistancePerLength"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#ResistancePerLength">
      <xs:annotation>
         <xs:documentation>Resistance (real part of impedance) per unit of length.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:float"/>
   </xs:simpleType>
   <xs:simpleType name="Seconds"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#Seconds">
      <xs:annotation>
         <xs:documentation>Time, in seconds.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:float"/>
   </xs:simpleType>
   <xs:simpleType name="Susceptance"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#Susceptance">
      <xs:annotation>
         <xs:documentation>Imaginary part of admittance.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:float"/>
   </xs:simpleType>
   <xs:simpleType name="SusceptancePerLength"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#SusceptancePerLength">
      <xs:annotation>
         <xs:documentation>Imaginary part of admittance per unit of length.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:float"/>
   </xs:simpleType>
   <xs:simpleType name="Voltage"
                  sawsdl:modelReference="http://cim.ucaiug.io/CIM101/draft#Voltage">
      <xs:annotation>
         <xs:documentation>Electrical voltage, can be both AC and DC.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:float"/>
   </xs:simpleType>
</xs:schema>
